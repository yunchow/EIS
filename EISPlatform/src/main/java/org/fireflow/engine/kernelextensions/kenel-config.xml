<?xml version="1.0" encoding="UTF-8"?>
<fireflow-config>
	<kenel>
		<extensions>
			<extension target="org.fireflow.kenel.ActivityInstance" 
					point="NodeInstanceEventListener" 
					class="org.fireflow.engine.kenelextensions.ActivityInstanceExtension">
			</extension>
			
			<extension target="org.fireflow.kenel.EndNodeInstance" 
					point="EndNodeInstanceEventListenser" 
					class="org.fireflow.engine.kenelextensions.EndNodeInstanceExtension">
			</extension>
			
			<extension target="org.fireflow.kenel.StartNodeInstance" 
					point="StartNodeInstanceEventListenser" 
					class="org.fireflow.engine.kenelextensions.StartNodeInstanceExtension">
			</extension>
			<extension target="org.fireflow.kenel.SynchronizerInstance" 
					point="SynchronizerInstanceEventListenser" 
					class="org.fireflow.engine.kenelextensions.SynchronizerInstanceExtension">
			</extension>	
			 		
		</extensions>
	</kenel>
</fireflow-config>